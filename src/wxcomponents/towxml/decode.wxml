    <view style="display: {{isShow[i]?'':'none !important'}};" class="h2w__videoParent"><video data-data="{{item}}" class="{{item.attrs.class}}" data="{{item.attrs.data}}" id="{{item.attrs.id}}" style="display: {{isShow[i]?'':'none !important'}};{{item.attrs.style}};" catch:tap="_tap" poster="{{item.attrs.poster}}" src="{{item.attrs.src}}">
        <decode wx:if="{{item.children}}" nodes="{{item}}" />
