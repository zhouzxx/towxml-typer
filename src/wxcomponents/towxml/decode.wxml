<block wx:for="{{nodes.children}}" wx:for-index="i" wx:for-item="item" wx:key="i" id="{{item.id}}">
  <block wx:if="{{item.tag===undefined}}">
    <typable-text text="{{item.text}}" textId="{{item.id}}" />
  </block>
  <block wx:if="{{item.tag==='view'}}">
    <block wx:if="{{item.rely}}">
      <view data-data="{{item}}" style="display: {{isShow[i]?'':'none !important'}};{{item.attrs.style}};" class="{{item.attrs.class}}" data="{{item.attrs.data}}" id="{{item.attrs.id}}"  catch:tap="_tap">
        <decode wx:if="{{item.children}}" nodes="{{item}}" />
      </view>
    </block>
    <block wx:else>
      <view class="h2w__viewParent" style="display: {{isShow[i]?'':'none !important'}};">
        <view data-data="{{item}}" class="{{item.attrs.class}}" data="{{item.attrs.data}}" id="{{item.attrs.id}}" style="display: {{isShow[i]?'':'none !important'}};{{item.attrs.style}};" catch:tap="_tap">
          <decode wx:if="{{item.children}}" nodes="{{item}}" />
        </view>
      </view>
    </block>
  </block>
