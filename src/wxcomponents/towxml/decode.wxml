      <rich-text data-data="{{item}}" class="{{item.attrs.class}}" data="{{item.attrs.data}}" style="display: {{isShow[i]?'':'none !important'}};{{item.attrs.style}};" catch:tap="_tap" nodes="{{item.children}}">
        <decode wx:if="{{item.children}}" nodes="{{item}}" />
      </rich-text>
    </view>
  </block>
  <block wx:if="{{item.tag==='audio-player'}}">
    <audio-player style="display: {{isShow[i]?'':'none !important'}};" data="{{item}}" data-data="{{item}}" catch:tap="_tap" />
  </block>
  <block wx:if="{{item.tag==='table'}}">
    <table style="display: {{isShow[i]?'':'none !important'}};" data="{{item}}" data-data="{{item}}" catch:tap="_tap" />
  </block>
  <block wx:if="{{item.tag==='todogroup'}}">
    <todogroup style="display: {{isShow[i]?'':'none !important'}};" data="{{item}}" data-data="{{item}}" catch:tap="_tap" />
